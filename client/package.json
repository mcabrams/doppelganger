{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "generate-env-config": "chmod +x ./env.sh && ./env.sh && cp env-config.js ./public/",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts,.tsx src/ --fix",
    "start": "concurrently \"npm run generate-env-config && webpack-dev-server --progress --color --mode development --config webpack.config.development.js\" \"npm run generate:watch\"",
    "prebuild": "npm run generate",
    "build": "chmod +x ./env.sh && ./env.sh && cp env-config.js ./public/ && webpack --color --mode production --config webpack.config.production.js",
    "upgrade-interactive": "npm-check --update",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
