    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.0",
    "@types/graphql": "14.2.3",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.4",
