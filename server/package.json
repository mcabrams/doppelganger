{
  "name": "server",
  "scripts": {
    "lint": "eslint --ext .js,.ts src/",
    "lint:fix": "eslint --ext .js,.ts src/ --fix",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "serve": "dotenv -- nodemon -L -e ts,graphql -x ts-node -r tsconfig-paths/register src/index.ts",
    "start": "concurrently \"npm run generate:watch\" \"npm run serve\"",
    "debug": "dotenv -- nodemon -L -e ts,graphql -x ts-node --inspect -r tsconfig-paths/register src/index.ts",
    "build": "rimraf dist && tsc",
    "pretest": "prisma deploy -e .test.env",
    "test": "jest"
  },
  "dependencies": {
    "apollo-link": "1.2.12",
    "apollo-link-http": "1.5.15",
    "apollo-server-express": "2.9.2",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "express": "4.17.1",
    "graphql": "14.3.1",
    "graphql-import": "0.7.1",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.5",
    "http": "0.0.0",
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "1.6.1",
    "@graphql-codegen/cli": "1.6.1",
    "@graphql-codegen/typescript": "1.5.0",
    "@graphql-codegen/typescript-resolvers": "1.5.0",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "0.0.36",
    "@types/express": "4.17.1",
    "@types/graphql": "14.2.0",
    "@types/graphql-depth-limit": "1.1.2",
    "@types/jest": "24.0.18",
    "@types/jsonwebtoken": "8.3.3",
    "@types/node": "12.7.3",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "concurrently": "4.1.2",
    "dotenv-cli": "1.4.0",
    "eslint": "6.3.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.16.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-import-node": "0.0.4",
    "jest": "24.9.0",
    "nodemon": "1.19.1",
    "prisma": "1.34.8",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.2.0",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.4.5"
  }
}
